<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="line_big_horizontal">
        <item name="android:layout_height">12dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/line_f0</item>
    </style>
    <style name="line_big_vertical">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">12dp</item>
        <item name="android:background">@color/line_f0</item>
    </style>
    <style name="line_horizontal">
        <item name="android:layout_height">0.4dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/line_ccc</item>
    </style>
    <style name="line_vertical">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0.4dp</item>
        <item name="android:background">@color/line_ccc</item>
    </style>
<!--    <style name="more_img">-->
<!--        <item name="android:src">@drawable/ic_more</item>-->
<!--        <item name="android:layout_width">24dp</item>-->
<!--        <item name="android:layout_height">24dp</item>-->
<!--    </style>-->
    <style name="text14" parent="TextAppearance.AppCompat.Body1">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text333</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="text16" parent="TextAppearance.AppCompat.Body1">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/text333</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


</resources>